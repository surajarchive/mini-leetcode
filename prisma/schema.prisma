
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model user {
  id   String  @id @default(uuid())
  name String?
  age  Int?
  posts post[]
}

model post {
  id        String   @id @default(uuid())
  title     String
  content   String?
  published Boolean  @default(false)
  authorId  String
  author    user     @relation(fields: [authorId], references: [id])
}




